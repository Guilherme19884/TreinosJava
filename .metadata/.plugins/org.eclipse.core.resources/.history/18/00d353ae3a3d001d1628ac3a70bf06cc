
public class Cliente {
	public String nome;
	public int idade;
	
	public void receberNome(String nome) {
		this.nome = nome;
	}
	public String mostrarNome () {
		return this.nome;
	}
	
	public void receberIdade(int idade) {
		this.idade = idade;
	}
	public int receberIdade() {
		return this.idade;
	}
	
}
